<div>
  <% if count_choice('a') > count_choice('b') %>
    <h2>Result: You are an Extrovert</h2>
  <%elsif count_choice('a') < count_choice('b')%>
    <h2>Result: You are an Introvert</h2>
  <%else%>
    <h2>Result: You are an Ambivert</h2>
  <% end %>
</div>

<div>
  <%= result_text(@result) %>
</div>

<div class="start">
  <a class="button" href="/">Start again</a>
</div>